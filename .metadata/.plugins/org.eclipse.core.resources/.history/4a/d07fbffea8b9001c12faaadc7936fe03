package Atelier5;

import java.util.Scanner;

public class Tache1 {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		
		int tab[];
		int i,N;
		
		System.out.println("Saisir le nombre d'element du tableau");
		N=s.nextInt();
		
		tab=new int[N];
		
		for(i=0;i<N;i++) {
			System.out.println("Element a la postion"+i);
			tab[i]=s.nextInt();		
		}
		
		System.out.println(tab[i]=i);
		
		
	}

}

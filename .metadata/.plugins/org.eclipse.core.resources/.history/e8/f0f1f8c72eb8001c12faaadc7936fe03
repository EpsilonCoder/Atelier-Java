package Tp1;

import java.util.Scanner;

public class Epsilon {

	public static void main(String[] args) {
		
		Scanner s=new Scanner(System.in);
		System.out.println("Enter le nom de l employé");
		String nom,id;
		
		nom=s.nextLine();
		id=s.nextLine();
        
        System.out.println("Entrer les heure effectué et le chiffre d affaire");
        long ca;
        
        ca=9.nextLong();
        
        Employe emp=new Employe(nom,id,ca);
        
        emp.commission();
        emp.salaire();
        emp.afficher();
        
	}

}

		System.out.println("Entrer le nombre de ligne");j=s.nextInt();

		String tab[]=new String[i];
		for (int x=0 ;x<i;x++) {
			for(int a=0;a<j;a++) {
				System.out.println("Entrer l'element ["+(i+1)+"]" +"["+(j+1)+"]");
			}
			
			
		}
		
		
		
		
	}

}

package Tp1;

public class Employe {
	private String nom;
	private String id;
	private int  ca;
	
	
	public Employe(String nom, String id,String ca) {
		this.nom=nom;
		this.id=id;
		this.ca=ca;
	}
	
	public double commission() {
		long commis;
		if(ca > 80000) {
			commis=(ca*25)/100;
		}
		else {
			commis=(ca*100)/100;
		}
		return commis;
	}
	
	public double salaire() {
		long salaire;
		int nh;
		salaire=nh*15000;
		long sal;
		salaire=commis+salaire;
	}
	

}

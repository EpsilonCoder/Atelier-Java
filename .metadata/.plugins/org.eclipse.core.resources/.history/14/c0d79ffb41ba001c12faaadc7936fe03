package Atelier6;
import java.util.*;
public class Collection1 {

	public static void main(String[] args) {
		int N;
		
		Scanner s=new Scanner(System.in);
		System.out.println("Entrer le nombre de livre a enregister");
		N=s.nextInt();
		
		int[] tab = new int[N];
		
		for (int i=0;i<N;i++) {
			System.out.println("Enter le livre N " +i);
			tab[i]=s.nextInt();	
		}
		
	

	}

}

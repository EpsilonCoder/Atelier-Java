package Atelier5;

import java.util.Scanner;

public class Tache1 {

	public static void main(String[] args) {
		int [] tab = new int [10] ;
		Scanner s=new Scanner(System.in);
		
		System.out.println("Enter les valeur du tableaux");
		int i = 0;
		tab[i]=s.nextInt();
	
		System.out.println(tab[i]);
		
		

	}

}

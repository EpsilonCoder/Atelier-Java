package Atelier5;

import java.util.Scanner;

public class Tache1 {

	public static void main(String[] args) {
		int i;
		int [] tab = new int [10] ;
		Scanner s=new Scanner(System.in);
		System.out.println("Enter les valeur du tableaux");
		tab[i]=s.nextInt();
	
		for (i=0;i<tab.length;i++) {	
			System.out.println(tab[i]=i);
		}
		
		

	}

}
